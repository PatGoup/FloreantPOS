<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.DeliveryInstruction" table="DELIVERY_INSTRUCTION">
		<id column="ID" name="id" type="java.lang.Integer">
      		<generator class="identity"/>
    	</id>
		<property name="notes" column="NOTES" length="220" type="string" />
		
		<many-to-one class="com.floreantpos.model.Customer" lazy="false" name="customer">
			<column name="CUSTOMER_NO" />
		</many-to-one>
	</class>
</hibernate-mapping>
