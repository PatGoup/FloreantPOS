<configuration>
 
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
<!--    <root level="trace">
    <root level="debug">
    <root level="info">
    <root level="warn">     -->
    <root level="error">
        <appender-ref ref="console"/>
    </root>
 
</configuration>

